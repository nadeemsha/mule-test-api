	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="aa13a2f0-2a1b-424b-947d-041ca2fe9102" >
		<http:listener-connection host="0.0.0.0" port="8081" />
	</http:listener-config>
	<flow name="anypoint-parallel-for-each-example-0Flow" doc:id="afc114ff-5965-4b48-9172-9dfa95350327" >
		<http:listener doc:name="Listener" doc:id="350381e0-8264-4588-a46f-82d19f83cb89" config-ref="HTTP_Listener_config" path="/example-1"/>
		<ee:transform doc:name="1 to 10" doc:id="984a4582-3453-49ad-953a-9c5963c4dd56" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/java
---
1 to 10]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<parallel-foreach doc:name="Parallel For Each" doc:id="76ca1d38-efd4-464e-a4f1-275529269101" >
