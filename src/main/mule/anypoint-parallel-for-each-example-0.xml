		<ee:transform doc:name="JSON" doc:id="ecfd05e8-59c0-4a46-8233-b6d5f2fab152" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
payload]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
</mule>
